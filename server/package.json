    "@prisma/client": "^3.12.0",
    "apollo-server": "^3.6.7",
    "apollo-server-core": "^3.7.0",
    "dotenv": "^16.0.0",
    "graphql": "^16.3.0",
    "jsonwebtoken": "^8.5.1",
    "jwks-rsa": "^2.1.0",
    "prisma": "^3.12.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.8",
